{
  "name": "Windowed",
  "version": "1.3.0",
  "description": "Instagram for the desktop",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.windowed.app",
    "productName": "Windowed",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "author": "Windowed.io",
  "keywords": [
    "Instagram",
    "desktop",
    "post",
    "photo",
    "mac",
    "pc"
  ],
  "devDependencies": {
    "electron": "~1.6.2",
    "electron-builder": "^22.8.0"
  },
  "dependencies": {
    "electron-google-analytics": "0.0.17",
    "font-awesome": "^4.7.0",
    "i": "^0.3.6",
    "node-localstorage": "^1.3.0",
    "npm": "^5.5.1"
  }
}
